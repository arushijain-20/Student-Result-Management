<a href="/" class="btn btn-info" style="margin-top:5px;">Logout</a>
<h4>My Result</h4>
<div class="stuTable">
  <table class="table table-striped">
      <thead>
        <tr>
          <th scope="col">Roll no.</th>
          <th scope="col">Name</th>
          <th scope="col">Course</th>
          <th scope="col">Score</th>
          <th scope="col">Attendance</th>
        </tr>
      </thead>
      <tbody>        
        <tr>
          <td><%= one.roll %> </td>
          <td><%= one.name %> </td>
          <td><%= one.course %> </td>
          <td><%= one.score %> </td>
          <td><%= one.attendance %> </td>
        </tr>
        
      </tbody>
    </table>    
</div>